<ul class="p-news__type">
    <mt:ArchiveList archive_type="Category-Yearly"> 
        <li class="widget-list-item"><a href="<$mt:ArchiveLink encode_html="1"$>" data-name="<mt:ArchiveDate format="%Y">"><$mt:ArchiveTitle$></a></li>
    </mt:ArchiveList>
</ul>

