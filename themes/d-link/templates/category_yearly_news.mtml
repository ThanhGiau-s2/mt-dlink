</button>

<div class="p-news__select js-select_menu">
<ul class="p-news__select_list js-select_list">
    <mt:ArchiveList archive_type="Category-Yearly">
        <li class="widget-list-item"><a href="<$mt:ArchiveLink encode_html="1"$>"><$mt:ArchiveTitle$></a></li>
    </mt:ArchiveList>
</ul>
</div>
<!-- /.p-news__select -->

<div class="c-content_heading">
<h1 class="c-content_heading__text"><$mt:ArchiveDate format="%Y"$>年</h1>
</div>
<!-- /.c-content_heading -->

<div class="p-news__content">
<div class="p-news__block--show">
<!--
# カテゴリHTML
<p class="p-news__category--news">お知らせ</p>
<p class="p-news__category--product">製品ニュース</p>
-->
<mt:PageContents count="20">
    <mt:SetVars>totalEntry=<$mt:BlogEntryCount$></mt:SetVars>
    <mt:Entries sort_by="modified_on" lastn="$totalEntry">
        <mt:EntryPrimaryCategory>
            <mt:HasNoParentCategory>
                <div class="p-news__item">
                    <a href="<$mt:EntryLink$>">
                        <div class="p-news__box">
                            <p class="p-news__time"><$mt:EntryDate format="%Y/%m/%d"$></p>
                            <mt:EntryAdditionalCategories glue="," setvar="sub_cats">
                                <$mt:CategoryLabel$>
                            </mt:EntryAdditionalCategories>
                            <mt:EntryAdditionalCategories setvar="cate_name"><$mt:CategoryBasename$></mt:EntryAdditionalCategories>
                            <mt:If name="cate_name" eq="info">
                                <p class="p-news__category--news"><$mt:GetVar name="sub_cats"$></p>
                            <mt:Else>
                                <p class="p-news__category--product"><$mt:GetVar name="sub_cats"$></p> 
                            </mt:If>
                            <p class="p-news__title"><$mt:EntryTitle$></p>
                        </div>
                        <!-- /.p-news__box -->
                    </a>
                </div>
                <!-- /.p-news__item -->
            </mt:HasNoParentCategory>
        </mt:EntryPrimaryCategory>
        <$mt:PageSeparator$>
    </mt:Entries>
</mt:PageContents>
<ul class="c-pager">
<!-- <li class="c-pager__prev"><a href="/news/2020.html">前へ</a></li> -->
<!-- <li class="c-pager__next"><span>次へ</span></li> -->
    <mt:Pagination>
        <mt:PaginationHeader>
            <mt:ifPaginationPrev>
                <li class="c-pager__prev"><a href="<mt:PaginationPrev>">&lt;前へ</a></li>
            </mt:ifPaginationPrev>
        </mt:PaginationHeader>
        <mt:ifPaginationCurrent>
            <li class="c-pager__number"><span><mt:PaginationLink element="number"></span></li>
        <mt:Else>
            <li class="c-pager__number"><a href="<mt:PaginationLink>"><mt:PaginationLink element="number"></a></li>
        </mt:ifPaginationCurrent>
        <mt:PaginationFooter>
            <mt:ifPaginationNext>
                <li class="c-pager__next"><a href="<mt:PaginationNext>">次へ</a></li>
            </mt:ifPaginationNext>
