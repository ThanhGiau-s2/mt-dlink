<mt:ParentCategories setvar="list_cate"><mt:CategoryBasename></mt:ParentCategories>
<mt:If name="list_cate" like="news">
    <mt:Include module="category_yearly_news" parent="1" />
<mt:Else>
    <mt:If name="list_cate" eq="supportrelease">
        <mt:Include module="category_yearly_support_release" parent="1" />
    </mt:If>
</mt:If>