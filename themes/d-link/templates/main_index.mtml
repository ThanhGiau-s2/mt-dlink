                                            <div class="p-product_category__info--small">
                                                <h3 class="p-product_category__name--small"><mt:EntryTitle></h3>
                                            </div>
                                            <!-- /.p-product_category__info--small -->

                                            <div class="p-product_category__image--small">
                                                <MTIfNonEmpty tag="EntryDataProducttopSum">
                                                    <mt:EntryDataProducttopSumAsset>
                                                        <img src="<mt:AssetURL>" alt="<mt:EntryTitle>">
                                                    </mt:EntryDataProducttopSumAsset>
                                                <mt:else>
                                                    <img src="/asset/img/product/alternative_image.jpg" alt="<mt:EntryTitle>">
                                                </MTIfNonEmpty>
                                            </div>
                                            <!-- /.p-product_category__image--small -->
                                        </div>
                                        <!-- /.p-product_category__row -->
                                    </a>
                                </div>
                                <!-- /.p-product_category__box -->
                            </mt:unless>
                        </mt:if>
                    </mt:Entries>
                    <div class="p-product_category__box">
                        <a href="/product/compare/" class="p-product_category__other">
                            <div class="p-product_category__row--other">
                                <div class="p-product_category__comparison">
                                    <div class="p-product_category__icon">
                                        <img src="/asset/img/top/category_icon_01.png" alt="">
                                    </div>
                                    <!-- /.p-product_category__icon -->
                                    <p class="p-product_category__name--other">製品を比較する</p>
                                </div>
                                <!-- /.p-product_category__comparison -->
                            </div>
                            <!-- /.p-product_category__row -->
                        </a>
                    </div>
                    <!-- /.p-product_category__box -->
                </div>
                <!-- /.p-product_category__small -->
            </div>
            <!-- /.p-product_category -->

            <mt:Ignore>** 製品に関する重要なお知らせ **</mt:Ignore>
            <mt:Entries category="サポート情報">
                <mt:If tag="EntryDataImportantTagSupport" eq="1">
                    <mt:setvarblock name="have_article_support">true</mt:setvarblock>
                </mt:If>
            </mt:Entries>
            <mt:If name="have_article_support" eq="true">
                <div class="p-emergency--product">
                    <div class="p-emergency__inner">
                        <div class="p-emergency__content--product">
                            <h2 class="p-emergency__heading--product">製品に関する重要なお知らせ</h2>
                            <ul class="p-emergency__list">
                                <mt:Entries field:important_tag_support="1" sort_by="authored_on" sort_order="descend" category="サポート情報" lastn="2">
                                    <li>
                                        <a href="<$mt:EntryPermalink$>" rel="bookmark">
