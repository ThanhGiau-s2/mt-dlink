                <!-- /.p-product_category__large -->

                <div class="p-product_category__small">
                    <mt:Entries category="製品情報" sort_by="authored_on" sort_order="ascend">
                        <mt:EntryCategories setvar="parent_cate"><$mt:CategoryLabel$></mt:EntryCategories>
                        <mt:if name="parent_cate" eq="製品情報">
