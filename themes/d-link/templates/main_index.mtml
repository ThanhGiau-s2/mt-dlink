    <section class="l-outer">
        <div class="l-inner">

            <div class="p-news">
                <ul class="p-news__tab js-news_tab">
                    <li><a href="#news" class="p-news__toggle">ニュース</a></li>
                    <li><a href="#event" class="p-news__toggle">イベント・<br class="u-show_sp">セミナー</a></li>
                    <li><a href="#support" class="p-news__toggle">サポート</a></li>
                </ul>

                <div class="p-news__content">
                    <div id="news" class="p-news__block">
                        <!--
                        # カテゴリHTML
                        <p class="p-news__category--news">お知らせ</p>
                        <p class="p-news__category--product">製品ニュース</p>
                        -->
                        <mt:Entries sort_by="authored_on" sort_order="descend" category="ニュース" lastn="5">
                            <mt:SetVarBlock name="cate_news"></mt:SetVarBlock>
                            <div class="p-news__item">
                                <a href="<$mt:EntryLink$>">
                                    <div class="p-news__box">
                                        <p class="p-news__time"><$mt:EntryDate format="%Y/%m/%d"$></p>
                                        <mt:EntryAdditionalCategories setvar="entry_categories"><mt:CategoryBasename></mt:EntryAdditionalCategories>
                                        <mt:If name="entry_categories">
                                            <mt:SubCategories category="ニュース">
                                                <mt:setvarblock name="id_name_cate"><mt:CategoryID>_<mt:CategoryBasename></mt:setvarblock>
                                                <mt:CategoryBasename setvar="name_cate">
                                                <mt:if name="entry_categories" like="$name_cate">
                                                    <mt:SetVarBlock name="cate_news{$id_name_cate}"><mt:CategoryLabel></mt:SetVarBlock>
                                                </mt:If>
                                            </mt:SubCategories>
                                            <mt:Loop name="cate_news" sort_by= "key numeric">
                                                <mt:If name="__last__">
                                                    <mt:If name="__key__" like="info">
                                                        <p class="p-news__category--news"><mt:Var name="__value__"></p>
                                                    </mt:If>
                                                    <mt:If name="__key__" like="product">
                                                        <p class="p-news__category--product"><mt:Var name="__value__"></p>
                                                    </mt:If>
                                                </mt:If>
                                            </mt:Loop>
                                        <mt:Else>
                                            <p class="p-news__category--news"></p>
                                        </mt:If>
                                        <p class="p-news__title"><$mt:EntryTitle$></p>
                                    </div>
                                    <!-- /.p-news__box -->
                                </a>
                            </div>
                            <!-- /.p-news__item -->
                        </mt:Entries>
                        <div class="c-button">
                            <a href="/news/">すべてのニュースを見る</a>
                        </div>

                    </div>
                    <!-- /.p-news__block -->

                    <div id="event" class="p-news__block">
                        <!--
                        # カテゴリHTML
                        <p class="p-news__category--event">イベント</p>
                        <p class="p-news__category--seminar">セミナー</p>
                        <p class="p-news__category--online">オンライン</p>
                        -->
                        <mt:Entries sort_by="authored_on" sort_order="descend" category="イベント・セミナー" lastn="5">
                            <mt:SetVarBlock name="cate_event"></mt:SetVarBlock>
                            <div class="p-news__item">
                                <a href="<$mt:EntryLink$>">
                                    <div class="p-news__box">
                                        <p class="p-news__time"><$mt:EntryDate format="%Y/%m/%d"$></p>
                                        <mt:EntryAdditionalCategories setvar="entry_categories"><mt:CategoryBasename></mt:EntryAdditionalCategories>
                                        <mt:If name="entry_categories">
                                            <mt:SubCategories category="イベント・セミナー">
                                                <mt:setvarblock name="id_name_cate"><mt:CategoryID>_<mt:CategoryBasename></mt:setvarblock>
                                                <mt:CategoryBasename setvar="name_cate">
                                                <mt:if name="entry_categories" like="$name_cate">
                                                    <mt:SetVarBlock name="cate_event{$id_name_cate}"><mt:CategoryLabel></mt:SetVarBlock>
                                                </mt:If>
                                            </mt:SubCategories>

                                            <mt:Loop name="cate_event" sort_by= "key numeric">
                                                <mt:If name="__last__">
                                                    <mt:If name="__key__" like="event-info">
                                                        <p class="p-news__category--event"><mt:Var name="__value__"></p>
                                                    </mt:If>
                                                    <mt:If name="__key__" like="online">
                                                        <p class="p-news__category--online"><mt:Var name="__value__"></p>
                                                    </mt:If>
                                                    <mt:If name="__key__" like="seminar">
                                                        <p class="p-news__category--seminar"><mt:Var name="__value__"></p>
                                                    </mt:If>
                                                </mt:If>
                                            </mt:Loop>
                                        <mt:Else>
                                            <p class="p-news__category--event"></p>
                                        </mt:If>
                                        <div class="p-news__body">
                                            <p class="p-news__title"><$mt:EntryTitle$></p>
                                            <p class="p-news__date">【日程】<$mt:EntryDataEventDate$></p>
                                            <p class="p-news__place">【会場】<$mt:EntryDataEventHall$></p>
                                        </div>
                                        <!-- /.p-news__body -->
                                    </div>
                                    <!-- /.p-news__box -->
                                </a>
                            </div>
                            <!-- /.p-news__item -->
                        </mt:Entries>
                        <div class="c-button">
                            <a href="/event/">すべてのイベント・セミナーを見る</a>
                        </div>

                    </div>
                    <!-- /.p-news__block -->

                    <div id="support" class="p-news__block">
                        <!--
                        # カテゴリHTML
                        <p class="p-news__category--support">お知らせ</p>
                        <p class="p-news__category--firmware">ファームウェア</p>
                        <p class="p-news__category--security">セキュリティ情報</p>
                        <p class="p-news__category--maintenance">メンテナンス情報</p>
                        <p class="p-news__category--failure">障害情報</p>
                        -->
                        <mt:Entries sort_by="authored_on" sort_order="descend" category="サポート/サポート情報" lastn="5">
                            <mt:SetVarBlock name="cate_support"></mt:SetVarBlock>
                            <div class="p-news__item">
                                <a href="<$mt:EntryLink$>">
                                    <div class="p-news__box">
                                        <p class="p-news__time"><$mt:EntryDate format="%Y/%m/%d"$></p>
                                        <mt:EntryAdditionalCategories setvar="entry_categories"><mt:If tag="CategoryBasename" ne="release"><$mt:CategoryBasename$></mt:If></mt:EntryAdditionalCategories>
                                        <mt:If name="entry_categories">
                                            <mt:SubCategories category="サポート/サポート情報">
                                                <mt:setvarblock name="id_name_cate"><mt:CategoryID>_<mt:CategoryBasename></mt:setvarblock>
                                                <mt:CategoryBasename setvar="name_cate">
                                                <mt:if name="entry_categories" like="$name_cate">
                                                    <mt:SetVarBlock name="cate_support{$id_name_cate}"><mt:CategoryLabel></mt:SetVarBlock>
                                                </mt:If>
                                            </mt:SubCategories>
                                            <mt:Loop name="cate_support" sort_by= "key numeric">
                                                <mt:If name="__last__">
                                                    <mt:If name="__key__" like="info">
                                                        <p class="p-news__category--support"><mt:Var name="__value__"></p>
                                                    </mt:If>
                                                    <mt:If name="__key__" like="fault">
                                                        <p class="p-news__category--failure"><mt:Var name="__value__"></p>
                                                    </mt:If>
                                                    <mt:If name="__key__" like="firmware">
                                                        <p class="p-news__category--firmware"><mt:Var name="__value__"></p>
                                                    </mt:If>
                                                    <mt:If name="__key__" like="security">
                                                        <p class="p-news__category--security"><mt:Var name="__value__"></p>
                                                    </mt:If>
                                                    <mt:If name="__key__" like="maintenance">
                                                        <p class="p-news__category--maintenance"><mt:Var name="__value__"></p>
                                                    </mt:If>
                                                </mt:If>
                                            </mt:Loop>
                                        <mt:Else>
                                            <p class="p-news__category--support"></p>
                                        </mt:If>
                                        <div class="p-news__body">
                                            <p class="p-news__title"><$mt:EntryTitle$></p>
                                        </div>
                                        <!-- /.p-news__body -->
                                    </div>
                                    <!-- /.p-news__box -->
                                </a>
                            </div>
                            <!-- /.p-news__item -->
                        </mt:Entries>
                        <div class="c-button">
                            <a href="/support/release/">すべてのサポート情報を見る</a>
                        </div>

                    </div>
                    <!-- /.p-news__block -->
                </div>
                <!-- /.p-news__content -->

            </div>
            <!-- /.p-news -->

        </div>
        <!-- /.l-inner -->
    </section>

</main>

