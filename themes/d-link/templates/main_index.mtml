<mt:SetVars>
    base_description=世界 71カ国、187 拠点を有する世界的なネットワーク機器メーカーD-Linkは、インターネット黎明期の1986年創業の老舗メーカーの1社です。エントリーレベルからハイエンドまでのスイッチ、無線LAN、ルータ、UTM、メディアコンバーター、IPカメラなど、ネットワーキングの全分野を網羅したネットワーク機器を、企画・製造・販売しております。D-Linkは 『 Building Networks for People 』 の理念の元、お客様と社会全体にネットワーク機器を通して貢献できる製品作りをグループ全体で行い、約670の製品を年間3,000万台以上世界各国に向けて出荷しています。日本では2005年7月にD-Linkブランド製品の販売を目的とした日本現地法人を設立。D-Link製品の中でも法人向け製品に特化して日本国内への浸透を図っています。
    og_title=ディーリンクジャパン株式会社 ｜ D-Link Japan ｜ Building Networks for People
    og_url=https://www.dlink-jp.com/
    og_type=website
</mt:SetVars>
<mt:SetVarTemplate name="file_javascript">
<script src="/asset/js/slick.min.js"></script>
<script src="/asset/js/top.min.js" defer></script>
</mt:SetVarTemplate>
<mt:Ignore>** 表示部分 **</mt:Ignore>
<mt:Include module="html_head" parent="1" />
<mt:Include module="header" parent="1" />
<main class="p-main">
    <div class="p-mv js-mv_slider">
        <mt:Entries category="トップスライダー">
            <mt:entrydatatopsliderBlock>
                <$mt:FLCFValue field="another_tab" setvar="another_tab"$>
                <$mt:FLCFValue field="link_url" setvar="link_url"$>
                <div class="p-mv__item">
                    <mt:if name="link_url">
                        <a href="<$mt:FLCFValue field="link_url"$>" <mt:if name="another_tab">target="_blank"</mt:if>>
                            <picture>
                                <mt:FLCFAsset field="photo_sp">
                                    <source srcset="<mt:AssetURL>" media="(max-width: 768px)">
                                </mt:FLCFAsset>
                                <mt:FLCFAsset field="photo_pc">
                                    <img src="<mt:AssetURL>" alt="<$mt:FLCFValue field="alt_text"$>">
                                </mt:FLCFAsset>
                            </picture>
                        </a>
                    <mt:else>
                        <picture>
                            <mt:FLCFAsset field="photo_sp">
                                <source srcset="<mt:AssetURL>" media="(max-width: 768px)">
                            </mt:FLCFAsset>
                            <mt:FLCFAsset field="photo_pc">
                                <img src="<mt:AssetURL>" alt="<$mt:FLCFValue field="alt_text"$>">
                            </mt:FLCFAsset>
                        </picture>
                    </mt:if>
                </div>
                <!-- /.p-mv__item -->
            </mt:entrydatatopsliderBlock>
        </mt:Entries>
    </div>
    <!-- /.p-mv -->

    <mt:Ignore>** 重要なお知らせ **</mt:Ignore>
    <mt:Entries category="ニュース">
        <mt:If tag="EntryDataImportantTag" eq="1">
            <mt:setvarblock name="have_article">true</mt:setvarblock>
        </mt:If>
    </mt:Entries>
    <mt:If name="have_article" eq="true">
        <div class="p-emergency">
            <div class="p-emergency__inner">
                <div class="p-emergency__content">
                    <h2 class="p-emergency__heading"><img src="/asset/img/common/icon_emergency_01.png" alt="">重要なお知らせ</h2>
                    <ul class="p-emergency__list">
                        <mt:Entries field:important_tag="1" sort_by="authored_on" sort_order="descend" category="ニュース" lastn="2">
                            <li>
                                <a href="<$mt:EntryLink$>">
                                    <div class="p-emergency__body">
                                        <p class="p-emergency__time"><$mt:EntryDate format="%Y/%m/%d"$></p>
                                        <p class="p-emergency__title"><$mt:EntryTitle$></p>
                                    </div>
                                </a>
                            </li>
                        </mt:Entries>
                    </ul>
                </div>
                <!-- /.p-emergency__content -->
            </div>
            <!-- /.p-emergency__inner -->
        </div>
        <!-- /.p-emergency -->
    </mt:if>
    
    <section class="l-outer--top">
        <div class="l-inner">
            <div class="c-heading">
                <h2 class="c-heading__title" data-en="category">製品カテゴリ一覧</h2>
            </div>
            <!-- /.c-heading -->

            <div class="p-product_category">
                <div class="p-product_category__large">
                    <mt:Entries category="製品情報">
                        <mt:if tag="EntryTitle" eq="スイッチ">
                            <div class="p-product_category__item">
                                <a href="/product/switch/">
                                    <div class="p-product_category__detail">
                                        <div class="p-product_category__info">
                                            <h3 class="p-product_category__name"><mt:EntryTitle></h3>
                                            <div class="p-product_category__introduction">
                                                <p><mt:EntryDataAllprotopText></p>
                                            </div>
                                            <!-- /.p-product_category__introduction -->
                                        </div>
                                        <!-- /.p-product_category__info -->
                                        <div class="p-product_category__image">
                                            <picture>
                                                <MTIfNonEmpty tag="EntryDataAlltopSum">
                                                    <mt:EntryDataAlltopSumAsset>
                                                        <source srcset="<mt:AssetURL>" media="(max-width: 768px)">
                                                    </mt:EntryDataAlltopSumAsset>
                                                <mt:else>
                                                    <source srcset="/asset/img/product/alternative_image.jpg" media="(max-width: 768px)">
                                                </MTIfNonEmpty>
                                                <MTIfNonEmpty tag="EntryDataProducttopSum">
                                                    <mt:EntryDataProducttopSumAsset>
                                                        <img src="<mt:AssetURL>" alt="<mt:EntryTitle>">
                                                    </mt:EntryDataProducttopSumAsset>
                                                <mt:else>
                                                    <img src="/asset/img/product/alternative_image.jpg" alt="<mt:EntryTitle>">
                                                </MTIfNonEmpty>
                                            </picture>
                                        </div>
                                        <!-- /.p-product_category__image -->
                                    </div>
                                    <!-- /.p-product_category__detail -->
                                </a>
                            </div>
                            <!-- /.p-product_category__item -->
                        </mt:if>
                    </mt:entries>
                    <mt:Entries category="製品情報">
                        <mt:if tag="EntryTitle" eq="無線LAN">
                            <div class="p-product_category__item">
                                <a href="/product/wireless-lan/">
                                    <div class="p-product_category__detail">
                                        <div class="p-product_category__info">
                                            <h3 class="p-product_category__name"><mt:EntryTitle></h3>
                                            <div class="p-product_category__introduction">
                                                <p><mt:EntryDataAllprotopText></p>
                                            </div>
                                            <!-- /.p-product_category__introduction -->
                                        </div>
                                        <!-- /.p-product_category__info -->
                                        <div class="p-product_category__image">
                                            <picture>
                                                <MTIfNonEmpty tag="EntryDataAlltopSum">
                                                    <mt:EntryDataAlltopSumAsset>
                                                        <source srcset="<mt:AssetURL>" media="(max-width: 768px)">
                                                    </mt:EntryDataAlltopSumAsset>
                                                <mt:else>
                                                    <source srcset="/asset/img/product/alternative_image.jpg" media="(max-width: 768px)">
                                                </MTIfNonEmpty>
                                                <MTIfNonEmpty tag="EntryDataProducttopSum">
                                                    <mt:EntryDataProducttopSumAsset>
                                                        <img src="<mt:AssetURL>" alt="<mt:EntryTitle>">
                                                    </mt:EntryDataProducttopSumAsset>
                                                <mt:else>
                                                    <img src="/asset/img/product/alternative_image.jpg" alt="<mt:EntryTitle>">
                                                </MTIfNonEmpty>
