<mt:SetVars>
    <mt:subcategories category="製品情報" setvar="subcate_0"><mt:CategoryBasename></mt:subcategories>
    <mt:EntryAdditionalCategories><mt:CategoryBasename setvar="cate_base"><mt:if name="subcate_0" like="$cate_base"><mt:CategoryBasename setvar="basename_max"><mt:CategoryLabel setvar="label_max"></mt:if></mt:EntryAdditionalCategories>
    <mt:setvarblock name="cate_1">製品情報/<mt:getvar name="label_max"></mt:setvarblock>
    <mt:subcategories category="$cate_1" setvar="subcate_1"><mt:CategoryBasename></mt:subcategories>
    <mt:EntryAdditionalCategories><mt:CategoryBasename setvar="cate_base">
        <mt:if name="cate_base" ne="$basename_max">
            <mt:if name="subcate_1" like="$cate_base">
                <mt:CategoryBasename setvar="basename_medium">
                <mt:CategoryLabel setvar="label_medium">
            <mt:else>
                <mt:CategoryBasename setvar="basename_small">
                <mt:CategoryLabel setvar="label_small">
            </mt:if>
        </mt:if>
    </mt:EntryAdditionalCategories>
    <mt:unless name="basename_medium"><mt:setvar name="basename_medium" value="$basename_max"></mt:unless>
    <mt:unless name="label_medium"><mt:setvar name="label_medium" value="$label_max"></mt:unless>
    <mt:unless name="basename_small"><mt:setvar name="basename_small" value="$basename_medium"></mt:unless>
    <mt:unless name="label_small"><mt:setvar name="label_small" value="$label_medium"></mt:unless>
    <mt:BlogEntryCount blog_ids="11" setvar="total_entries_series">
    <mt:If tag="EntryExcerpt">base_description=<$mt:EntryExcerpt$></mt:If>
    <mt:If tag="EntryKeywords">base_keywords=<$mt:EntryKeywords$></mt:If>
    og_title=<mt:EntryTitle>|<mt:getvar name="label_small">|<mt:getvar name="label_max">|製品情報 | D-Link Japan
    og_url=https://www.dlink-jp.com/product/switch/
    og_type=article
</mt:SetVars>
<mt:SetVarTemplate name="file_javascript">
<script src="/asset/js/vue.min.js"></script>
<mt:if name="basename_max" eq="switch">
<script src="/asset/js/product_switch_compare.min.js" defer></script>
</mt:if>
<mt:if name="basename_max" eq="wireless-lan">
<script src="/asset/js/product_wireless_compare.min.js" defer></script>
</mt:if>
</mt:SetVarTemplate>
<mt:Ignore>** 表示部分 **</mt:Ignore>
<mt:Include module="html_head" parent="1" />
<mt:Include module="header" parent="1" />
<!-- ここからコンテンツ -->
<main class="p-main">
    <div class="c-breadcrumb">
        <ul class="c-breadcrumb__list">
            <li><a href="/">HOME</a></li>
            <li><a href="/product/">製品情報</a></li>
            <li><a href="/product/<mt:getvar name="basename_max">/"><mt:getvar name="label_max"></a></li>
            <mt:if name="basename_max" eq="wireless-lan"><li><a href="/product/<mt:getvar name="basename_max">/<mt:getvar name="basename_medium">.html"><mt:getvar name="label_medium"></a></li></mt:if>
            <li><a href="/product/<mt:getvar name="basename_max">/<mt:getvar name="basename_medium">/<mt:getvar name="basename_small">/"><mt:getvar name="label_small"></a></li>
            <li><span><mt:EntryTitle></span></li>
        </ul>
    </div>
    <!-- /.c-breadcrumb -->
    <section id="compare_app" class="l-outer--lower">
        <div class="l-inner">
            <div class="p-product_main">
                <div class="p-product_main__header">
                <mt:Entries include_blogs="11" sort_by="authored_on" sort_order="descend" lastn="$total_entries_series">
                    <mt:if tag="EntryTitle" eq="$label_small">
                        <p class="p-product_main__series"><mt:if tag="EntryDataSeriesMiddlecatText"><mt:EntryDataSeriesMiddlecatText><br></mt:if><mt:EntryTitle></p>
                    </mt:if>
                </mt:entries>
                    <div class="p-product_main__title">
                        <h1 class="p-product_main__name"><mt:EntryTitle></h1>
                        <mt:If tag="EntryDataSeriesDetailSalestatus" like="(販売終了予定|販売終了)">
                            <div class="p-product_main__status--close"><mt:EntryDataSeriesDetailSalestatus></div>
                        <mt:else>
                            <$mt:DateDiff tag="EntryDate" offset="90d" format="%Y%m%d%H%M%S" setvar="exp_date"$>
                            <$mt:Date format="%Y%m%d%H%M%S" setvar="current_date"$>
                            <mt:If name="exp_date" gt="$current_date"><div class="p-product_main__status--new">NEW</div></mt:If>
                        </mt:If>
                    </div>
                    <!-- /.p-product_main__title -->
                    <div class="p-product_main__image">
                    <MTIfNonEmpty tag="EntryDataSeriesDetailMainImg">
                        <mt:EntryDataSeriesDetailMainImgAsset>
                            <img src="<mt:AssetURL>" alt="<mt:EntryTitle>">
                        </mt:EntryDataSeriesDetailMainImgAsset>
                    <mt:else>
                        <img src="/asset/img/product/alternative_image.jpg" alt="<mt:EntryTitle>">
                    </MTIfNonEmpty>
                    </div>
                    <!-- /.p-product_main__image -->
                    <mt:if name="basename_max" like="(switch|wireless-lan)">
                        <mt:if tag="EntryDataSeriesDetailSalestatus" ne="販売終了">
                            <mt:unless tag="EntryDataSeriesDetailOptionProduct">
                                <div class="p-product_main__checkbox">
                                    <div class="c-check">
                                        <input type="checkbox" name="compare_list[]" value="<mt:EntryID>" id="product_id_<mt:EntryID>" data-product_id="<mt:EntryID>" @change="add_compare_list" :checked="compare_check(<mt:EntryID>)">
                                        <label for="product_id_<mt:EntryID>">比較リストに追加する</label>
                                    </div>
                                    <!-- /.c-check -->
                                </div>
                                <!-- /.p-product_main__checkbox -->
                            </mt:unless>
                        </mt:if>
                    </mt:if>
                </div>
                <!-- /.p-product_main__header -->

                <div class="p-product_main__body">
                    <div class="p-information">
                        <div class="p-information__table">
                            <table>
                                <tbody>
                                    <mt:setvarblock name="detailproduct"><mt:EntryDataSeriesDetailProduct01><mt:EntryDataSeriesDetailProduct02><mt:EntryDataSeriesDetailProduct03></mt:setvarblock>
                                    <mt:if name="detailproduct">
                                    <tr>
                                        <th>品番</th>
                                        <td>
                                            <ul>
                                                <li><mt:EntryDataSeriesDetailProduct01></li>
                                                <li><mt:EntryDataSeriesDetailProduct02></li>
                                                <li><mt:EntryDataSeriesDetailProduct03></li>
                                            </ul>
                                        </td>
                                    </tr>
                                    </mt:if>
                                    <mt:if tag="EntryDataSeriesDetailHardware">
                                        <tr>
                                            <th>ハードウェア<br class="u-show_sp">バージョン</th>
                                            <td>
                                                <p><mt:EntryDataSeriesDetailHardware></p>
                                            </td>
                                        </tr>
                                    </mt:if>
                                    <mt:setvarblock name="standardprice"><mt:EntryDataSeriesDetailStandardPrice01><mt:EntryDataSeriesDetailStandardPrice02><mt:EntryDataSeriesDetailStandardPrice03></mt:setvarblock>
                                    <mt:if name="standardprice">
                                    <tr>
                                        <th>標準価格（税別）</th>
                                        <td>
                                            <ul class="u-fw_bold">
                                                <li><mt:EntryDataSeriesDetailStandardPrice01></li>
                                                <li><mt:EntryDataSeriesDetailStandardPrice02></li>
                                                <li><mt:EntryDataSeriesDetailStandardPrice03></li>
                                            </ul>
                                        </td>
                                    </tr>
                                    </mt:if>
                                    <mt:if tag="EntryDataSeriesDetailProductWarranty">
                                    <tr>
                                        <th>製品保証</th>
                                        <td>
                                            <p><mt:EntryDataSeriesDetailProductWarranty></p>
                                        </td>
                                    </tr>
                                    </mt:if>
                                    <mt:unless tag="EntryDataSeriesDetailSalestatus" like="(販売終了予定|販売終了)">
                                    <mt:if tag="EntryDataSeriesDetailSaleStartdate">
                                    <tr>
                                        <th>販売開始日</th>
                                        <td>
                                            <p><mt:EntryDataSeriesDetailSaleStartdate></p>
                                        </td>
                                    </tr>
                                    </mt:if>
                                    </mt:unless>
                                    <mt:If tag="EntryDataSeriesDetailSalestatus" like="(販売終了予定|販売終了)">
                                        <mt:if tag="EntryDataSeriesDetailSaleEnddate">
                                        <tr>
                                        <th>製造終了日<br class="u-show_sp">（EOL）</th>
                                        <td>
                                            <p><mt:EntryDataSeriesDetailSaleEnddate></p>
                                        </td>
                                        </tr>
                                        </mt:if>
                                        <mt:if tag="EntryDataSeriesDetailSupportEnddate">
                                        <tr>
                                            <th>サポート終了日<br class="u-show_sp">（EoSL）</th>
                                            <td>
                                                <p><mt:EntryDataSeriesDetailSupportEnddate></p>
                                            </td>
                                        </tr>
                                        </mt:if>
                                    </mt:if>
                                    
                                        <mt:setvarblock name="successormode"><mt:EntryDataSeriesDetailSuccessormodelText01><mt:EntryDataSeriesDetailSuccessormodelText02><mt:EntryDataSeriesDetailSuccessormodelText03></mt:setvarblock>
                                        <mt:if name="successormode">
                                        <tr>
                                            <th>後継機種品番</th>
                                            <td>
                                                <ul>
                                                    <li>
                                                        <mt:if tag="EntryDataSeriesDetailSuccessormodelUrl01">
                                                            <a href="<mt:EntryDataSeriesDetailSuccessormodelUrl01>">
                                                                <mt:EntryDataSeriesDetailSuccessormodelText01>
                                                            </a>
                                                        <mt:else>
                                                            <mt:EntryDataSeriesDetailSuccessormodelText01>
                                                        </mt:if>
                                                    </li>
                                                    <li>
                                                        <mt:if tag="EntryDataSeriesDetailSuccessormodelUrl02">
                                                            <a href="<mt:EntryDataSeriesDetailSuccessormodelUrl02>">
                                                                <mt:EntryDataSeriesDetailSuccessormodelText02>
                                                            </a>
                                                        <mt:else>
                                                            <mt:EntryDataSeriesDetailSuccessormodelText02>
                                                        </mt:if>
                                                    </li>
                                                    <li>
                                                        <mt:if tag="EntryDataSeriesDetailSuccessormodelUrl03">
                                                            <a href="<mt:EntryDataSeriesDetailSuccessormodelUrl03>">
                                                                <mt:EntryDataSeriesDetailSuccessormodelText03>
                                                            </a>
                                                        <mt:else>
                                                            <mt:EntryDataSeriesDetailSuccessormodelText03>
                                                        </mt:if>
                                                    </li>
                                                </ul>
                                            </td>
                                        </tr>
                                        </mt:if>
                                    
                                </tbody>
                            </table>
                        </div>
                        <!-- /.p-information__table -->

                        <div class="p-information__slot">
                            <!-- 01 -->
                            <mt:if tag="EntryDataSeriesDetailPortLeft01">
                            <div class="p-information__item">
                                <div class="p-information__row">
                                    <div class="p-information__name">
                                        <p><mt:EntryDataSeriesDetailPortLeft01></p>
                                    </div>
                                    <!-- /.p-information__name -->
                                    <div class="p-information__number">
                                        <p><mt:unless name="basename_max" eq="wireless-lan">×</mt:unless><mt:EntryDataSeriesDetailPortRight01></p>
                                    </div>
                                    <!-- /.p-information__number -->
                                </div>
                                <!-- /.p-information__row -->
                            </div>
                            <!-- /.p-information__item -->
                            </mt:if>
                            <!-- 02 -->
                            <mt:if tag="EntryDataSeriesDetailPortLeft02">
                            <div class="p-information__item">
                                <div class="p-information__row">
                                    <div class="p-information__name">
                                        <p><mt:EntryDataSeriesDetailPortLeft02></p>
                                    </div>
                                    <!-- /.p-information__name -->
                                    <div class="p-information__number">
                                        <p>×<mt:EntryDataSeriesDetailPortRight02></p>
                                    </div>
                                    <!-- /.p-information__number -->
                                </div>
                                <!-- /.p-information__row -->
                            </div>
                            <!-- /.p-information__item -->
                            </mt:if>
                            <!-- 03 -->
                            <mt:if tag="EntryDataSeriesDetailPortLeft03">
                            <div class="p-information__item">
                                <div class="p-information__row">
                                    <div class="p-information__name">
                                        <p><mt:EntryDataSeriesDetailPortLeft03></p>
                                    </div>
                                    <!-- /.p-information__name -->
                                    <div class="p-information__number">
                                        <p>×<mt:EntryDataSeriesDetailPortRight03></p>
                                    </div>
                                    <!-- /.p-information__number -->
                                </div>
                                <!-- /.p-information__row -->
                            </div>
                            <!-- /.p-information__item -->
                            </mt:if>
                            <!-- 04 -->
                            <mt:if tag="EntryDataSeriesDetailPortLeft04">
                            <div class="p-information__item">
                                <div class="p-information__row">
                                    <div class="p-information__name">
                                        <p><mt:EntryDataSeriesDetailPortLeft04></p>
                                    </div>
                                    <!-- /.p-information__name -->
                                    <div class="p-information__number">
                                        <p>×<mt:EntryDataSeriesDetailPortRight04></p>
                                    </div>
                                    <!-- /.p-information__number -->
                                </div>
                                <!-- /.p-information__row -->
                            </div>
                            <!-- /.p-information__item -->
                            </mt:if>
                            <!-- 05 -->
                            <mt:if tag="EntryDataSeriesDetailPortLeft05">
                            <div class="p-information__item">
                                <div class="p-information__row">
                                    <div class="p-information__name">
                                        <p><mt:EntryDataSeriesDetailPortLeft05></p>
                                    </div>
                                    <!-- /.p-information__name -->
                                    <div class="p-information__number">
                                        <p>×<mt:EntryDataSeriesDetailPortRight05></p>
                                    </div>
                                    <!-- /.p-information__number -->
                                </div>
                                <!-- /.p-information__row -->
                            </div>
                            <!-- /.p-information__item -->
                            </mt:if>
                            <!-- 06 -->
                            <mt:if tag="EntryDataSeriesDetailPortLeft06">
                            <div class="p-information__item">
                                <div class="p-information__row">
                                    <div class="p-information__name">
                                        <p><mt:EntryDataSeriesDetailPortLeft06></p>
                                    </div>
                                    <!-- /.p-information__name -->
                                    <div class="p-information__number">
                                        <p>×<mt:EntryDataSeriesDetailPortRight06></p>
                                    </div>
                                    <!-- /.p-information__number -->
                                </div>
                                <!-- /.p-information__row -->
                            </div>
                            <!-- /.p-information__item -->
                            </mt:if>
                        </div>
                        <!-- /.p-information__slot -->
                    </div>
                    <!-- /.p-information -->
                </div>
                <!-- /.p-product_main__body -->
            </div>
            <!-- /.p-product_main -->
            
            <div class="l-container">
                <div class="l-container__block">
                    <div class="l-container__box">
                        <div class="l-container__slot">
                            <mt:setvarblock name="icon"><mt:EntryDataSeriesDetailCharacteristicIcon><mt:EntryDataSeriesDetailSoftwareIcon><mt:EntryDataSeriesDetailHardwareIcon></mt:setvarblock>
                            <mt:if name="icon">
                                <div class="p-icon">
                                    <p class="p-icon__heading">機能アイコン</p>
                                    <ul class="p-icon__list">
                                        <mt:EntryDataSeriesDetailCharacteristicIcon replace="$","<li class="p-icon__item--yellow"><span>">
                                        <mt:EntryDataSeriesDetailSoftwareIcon replace="$","<li class="p-icon__item--green"><span>">
                                        <mt:EntryDataSeriesDetailHardwareIcon replace="$","<li class="p-icon__item--blue"><span>">
                                    </ul>
                                </div>
                                <!-- /.p-icon -->
                            </mt:if>
                            <div class="c-content_box u-mt10 u-color_notice">
                                <p><mt:EntryDataSeriesDetailFunctionIcon nl2br="1"></p>
                            </div>

                            <div class="p-button_2col">
                                <mt:if tag="EntryDataSeriesDetailFaqUrl">
