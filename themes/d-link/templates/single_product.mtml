<mt:SetVars>
    <mt:subcategories category="製品情報" setvar="subcate_0"><mt:CategoryBasename></mt:subcategories>
    <mt:EntryAdditionalCategories><mt:CategoryBasename setvar="cate_base"><mt:if name="subcate_0" like="$cate_base"><mt:CategoryBasename setvar="basename_max"><mt:CategoryLabel setvar="label_max"></mt:if></mt:EntryAdditionalCategories>
    <mt:setvarblock name="cate_1">製品情報/<mt:getvar name="label_max"></mt:setvarblock>
    <mt:subcategories category="$cate_1" setvar="subcate_1"><mt:CategoryBasename></mt:subcategories>
    <mt:EntryAdditionalCategories><mt:CategoryBasename setvar="cate_base">
        <mt:if name="cate_base" ne="$basename_max">
            <mt:if name="subcate_1" like="$cate_base">
                <mt:CategoryBasename setvar="basename_medium">
                <mt:CategoryLabel setvar="label_medium">
            <mt:else>
                <mt:CategoryBasename setvar="basename_small">
                <mt:CategoryLabel setvar="label_small">
            </mt:if>
        </mt:if>
    </mt:EntryAdditionalCategories>
    base_description=ディスクリプション
    og_title=<mt:EntryTitle>|<mt:getvar name="label">|<mt:getvar name="label_series">|製品情報 | D-Link Japan
    og_url=https://www.dlink-jp.com/product/switch/
    og_type=article
</mt:SetVars>
<mt:SetVarTemplate name="file_javascript">
<script src="/asset/js/vue.min.js"></script>
<mt:if name="basename_max" eq="switch">
<script src="/asset/js/product_switch_compare.min.js" defer></script>
</mt:if>
<mt:if name="basename_max" eq="wireless-lan">
<script src="/asset/js/product_wireless_compare.min.js" defer></script>
</mt:if>
</mt:SetVarTemplate>
<mt:Ignore>** 表示部分 **</mt:Ignore>
