                        <mt:else>
                            <$mt:DateDiff tag="EntryDate" offset="90d" format="%Y%m%d%H%M%S" setvar="exp_date"$>
                            <$mt:Date format="%Y%m%d%H%M%S" setvar="current_date"$>
                            <mt:If name="exp_date" gt="$current_date"><p class="p-product_list__status--new">NEW</p></mt:If>
                        </mt:If>
                        <!-- <div class="p-product_main__status--close">販売終了予定</div> -->
                        </div>
