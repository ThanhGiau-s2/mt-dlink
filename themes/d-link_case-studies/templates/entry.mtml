  <mt:Entries category="$parent_category" sort_by="authored_on" sort_order="descend"><mt:EntriesCount setvar="total_entries"></mt:entries>
  <mt:unless name="total_entries" eq="1">
  <div class="p-recommend">
    <div class="p-recommend__inner">
      <div class="p-recommend__header">
        <!--
          #news category
            <p class="p-recommend__category--news">お知らせ</p>
            <p class="p-recommend__category--product">製品ニュース</p>
          
          #event category
            <p class="p-recommend__category--event">イベント</p>
            <p class="p-recommend__category--seminar">セミナー</p>
            <p class="p-recommend__category--online">オンライン</p>
          
          #support category
            <p class="p-recommend__category--support">お知らせ</p>
            <p class="p-recommend__category--firmware">ファームウェア</p>
            <p class="p-recommend__category--security">セキュリティ情報</p>
            <p class="p-recommend__category--maintenance">メンテナンス情報</p>
            <p class="p-recommend__category--failure">障害情報</p>
          
          #case category
          <p class="p-recommend__category--case">導入事例</p>

          #solution category
          <p class="p-recommend__category--solution">ソリューションのご提案</p>
        -->
        <p class="p-recommend__category--case"><mt:getvar name="parent_category"></p>
        <h4 class="p-recommend__title">その他の導入事例</h4>
      </div>
      <!-- /.p-recommend__header -->
      <div class="p-recommend__body">
        <mt:Entries category="$parent_category" sort_by="authored_on" sort_order="descend" lastn="4">
          <mt:if tag="EntryTitle" eq="$current_article">
            <mt:SetVar name="have_current" value="1">
          </mt:if>
        </mt:entries>
        <mt:if name="have_current" eq="1">
          <mt:setvar name="number_of_article" value="4">
        <mt:else>
          <mt:SetVar name="number_of_article" value="3">
        </mt:if>
        
        <mt:Entries category="$parent_category" sort_by="authored_on" sort_order="descend" lastn="$number_of_article">
          <mt:unless tag="EntryTitle" eq="$current_article">
            <div class="p-recommend__item">
              <a href="<mt:EntryPermalink>">
                <div class="p-recommend__block">
                  <div class="p-recommend__img">
                    <MTIfNonEmpty tag="EntryDataCaseStudiesDetailListImage">
                      <mt:EntryDataCaseStudiesDetailListImageAsset>
                        <img src="<mt:AssetURL>" alt="<mt:EntryTitle>">
                      </mt:EntryDataCaseStudiesDetailListImageAsset>
                    <mt:else>
                      <img src="/asset/img/product/alternative_image.jpg" alt="<mt:EntryTitle>">
                    </MTIfNonEmpty>
                  </div>
                  <!-- /.p-recommend__img -->
                  <div class="p-recommend__content">
                    <div class="p-recommend__text">
                      <p><mt:EntryTitle></p>
                    </div>
                    <!-- /.p-recommend__text -->
                  </div>
                  <!-- /.p-recommend__content -->
                </div>
                <!-- /.p-recommend__block -->
              </a>
            </div>
            <!-- /.p-recommend__item -->
          </mt:unless>
        </mt:entries>
      </div>
      <!-- /.p-recommend__body -->
    </div>
    <!-- /.p-recommend__inner -->
  </div>
  <!-- /.p-recommend -->
  </mt:unless>
