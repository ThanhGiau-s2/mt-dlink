        </ul>
    </nav>

<div class="l-container">
    <mt:toplevelcategories>
        <mt:setvarblock name="sub_cate"><mt:CategoryLabel></mt:setvarblock>
        <div class="l-container__block">
            <div class="c-content_heading">
                <h2 id="<mt:CategoryBasename>" class="c-content_heading__text"><mt:CategoryLabel></h2>
            </div>
            <!-- /.c-content_heading -->

            <div class="l-container__box">
                <div class="p-media">
                    <div class="p-media__content">
                    <mt:Entries category="$sub_cate">
                        <div class="p-media__item">
                            <a href="<$mt:EntryPermalink$>">
                                <div class="p-media__detail">
                                    <div class="p-media__header">
                                        <h3 class="p-media__title"><mt:EntryTitle></h3>
                                        <div class="p-media__description">
                                            <p><mt:EntryDataCaseStudiesDetailListText></p>
                                        </div>
                                    </div>
                                    <div class="p-media__image">
                                        <mt:EntryDataCaseStudiesDetailListImageAsset>
                                            <img src="<mt:AssetURL>" alt="<mt:EntryTitle>">
                                        </mt:EntryDataCaseStudiesDetailListImageAsset>
                                    </div>
                                </div>
                            </a>
                        </div>
                        <!-- /.p-media__item -->
                    </mt:Entries>
                    </div>
                    <!-- /.p-media__content -->
                </div>
                <!-- /.p-media -->
            </div>
            <!-- /.l-container__box -->
        </div>
        <!-- /.l-container__block -->
    </mt:toplevelcategories>
    
    
    

