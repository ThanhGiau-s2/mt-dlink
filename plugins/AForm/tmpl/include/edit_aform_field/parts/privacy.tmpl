<div class="view-block">
	<div class="aform-input-example parts-%%field_idx%%">%%field_input_example%%</div>
	<ul class="aform-checkbox-ul aform-field-%%field_idx%% parts-%%field_idx%% aform-vertical-ul"></ul>
</div>

<mt:include name="include/edit_aform_field/parts_common.tmpl"
	display_label="1"
	display_parts_id="1"
	display_required="1"
	force_required="1"
	display_input_example="1">

<div class="form-group">
	<label for="aformFieldEditPrivacyLinkText%%field_idx%%"><__trans phrase="privacy_link"></label>
	<input type="text" class="form-control aform-field-edit-privacy-link-text" id="aformFieldEditPrivacyLinkText%%field_idx%%">
</div>

<div class="form-group">
	<label for=""><__trans phrase="Label for checkbox"></label>
	<div class="mt-table--outline mb-3">
		<table class="table mt-table values-option-table" ref="table">
			<thead>
				<tr>
					<th scope="col" class="check"><__trans phrase="Selected state"></th>
					<th scope="col"><__trans phrase="Label"></th>
				</tr>
			</thead>
			<tbody>
				<tr>
					<td><span data-tooltip="<__trans phrase="privacy policy warning">"><input type="checkbox" class="form-control aform-field-value-checkbox" name="" id="aformField%%field_idx%%-0"></span></td>
					<td><input type="text" class="form-control required aform-field-edit-value-label-text" id="aformFieldEditValueLabelText%%field_idx%%-0" name="label" value="" ></td>
				</tr>
			</tbody>
		</table>
	</div>
</div>
