<__trans_section component="AForm">

<div id="aform-<$mt:var name="id"$>" class="aform <$mt:var name="aform_lang"$>">
<div class="aform-header"><$mt:var name="aform_title"$></div>
<div class="aform-content">
<dl class="aform-error-list">
  <dt><__trans phrase="An error occurred."></dt>
    <mt:loop name="errors">
      <dd><mt:var name="__value__" escape="html"></dd>
    </mt:loop>
</dl>
<mt:ignore>
<div><input type="button" value="<__trans phrase="Back">" onclick="history.back();" /></div>
</mt:ignore>
<div><__trans phrase="Please return your browser's back button."></div>
</div>
</div>
</__trans_section>
